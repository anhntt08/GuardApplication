<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item
    android:bottom="1dp"
    android:left="1dp"
    android:right="1dp"
    android:top="1dp">
    <shape android:shape="rectangle" >
        <stroke
            android:color="#dedddd"
            android:dashWidth="60px"
            android:dashGap="10px"
            android:width="2dp"/>


        <padding
            android:top="5dp"
            android:bottom="10dp" />
    </shape>
</item>

</layer-list>